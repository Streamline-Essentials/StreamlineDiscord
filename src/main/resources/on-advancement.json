    {
      "name": "Description",
      "value": "%this_advancement_description%",
      "inline": false
    },
    {
      "name": "Criteria",
      "value": "%this_advancement_criteria%",
      "inline": false
    }
  ],
  "color": 16777215,
  "footer": {
    "text": "Powered by StreamlineCore.",
    "icon_url": "https://raw.githubusercontent.com/Streamline-Essentials/StreamlineWiki/main/s.png"
